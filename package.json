{
  "name": "a-lot-of-ts",
  "version": "0.0.1",
  "description": "",
  "license": "UNLICENSED",
  "scripts": {
    "----   all  ----": "",
    "start:debug": "concurrently \"npm run start:debug:*\" ",
    "prisma:generate": "concurrently \"npm run prisma:generate:*\" ",
    "prisma:deploy": "concurrently \"npm run migrate:deploy:*\" ",
    "----  admin ui ----": "",
    "start:debug:admin_api": "npx dotenv -e ./apps/admin_api/.env -- nest start admin_api --debug --watch",
    "build:admin_api": "nest build admin_api",
    "start:prd:admin_api": "node dist/apps/admin_api/main",
    "----  gateway ----": "",
    "start:debug:gateway": "npx dotenv -e ./apps/gateway/.env -- nest start gateway --debug --watch",
    "build:gateway": "nest build gateway",
    "start:prd:gateway": "node dist/apps/gateway/main",
    "----  player ----": "",
    "start:debug:player": "npx dotenv -e ./apps/player/.env -- nest start player --debug --watch",
    "prisma:generate:player": "npx dotenv -e ./apps/player/.env -- npx prisma generate --schema ./apps/player/src/utils/prisma/player.prisma",
    "migrate:dev:create:player": "npx dotenv -e ./apps/player/.env -- npx prisma migrate dev --create-only --schema ./apps/player/src/utils/prisma/player.prisma",
    "migrate:deploy:player": "npx dotenv -e ./apps/player/.env -- npx prisma migrate deploy --preview-feature --schema ./apps/player/src/utils/prisma/player.prisma",
    "migrate:reset:player": "npx dotenv -e ./apps/player/.env -- npx prisma migrate reset --preview-feature --schema ./apps/player/src/utils/prisma/player.prisma",
    "build:player": "nest build player",
    "start:prd:player": "npx prisma migrate deploy --preview-feature --schema ./dist/apps/player/utils/prisma/player.prisma && node dist/apps/player/main",
    "----   daily_bonus    ----": "",
    "start:debug:daily_bonus": "npx dotenv -e ./apps/daily_bonus/.env -- nest start daily_bonus --debug --watch",
    "prisma:generate:daily_bonus": "npx dotenv -e ./apps/daily_bonus/.env -- npx prisma generate --schema ./apps/daily_bonus/src/utils/prisma/daily-bonus.prisma",
    "migrate:dev:create:daily_bonus": "npx dotenv -e ./apps/daily_bonus/.env -- npx prisma migrate dev --create-only --schema ./apps/daily_bonus/src/utils/prisma/daily-bonus.prisma",
    "migrate:deploy:daily_bonus": "npx dotenv -e ./apps/daily_bonus/.env -- npx prisma migrate deploy --preview-feature --schema ./apps/daily_bonus/src/utils/prisma/daily-bonus.prisma",
    "migrate:reset:daily_bonus": "npx dotenv -e ./apps/daily_bonus/.env -- npx prisma migrate reset --preview-feature --schema ./apps/daily_bonus/src/utils/prisma/daily-bonus.prisma",
    "build:daily_bonus": "nest build daily_bonus",
    "start:prd:daily_bonus": "npx prisma migrate deploy --preview-feature --schema ./dist/apps/daily_bonus/utils/prisma/daily_bonus.prisma && node dist/apps/daily_bonus/main",
    "----   db    ----": "",
    "db:generate": "npm run prisma:generate:player",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/apps/gateway/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/gateway/test/jest-e2e.json",
    "----  prod  ----": "",
    "run_from_env": "sh -c 'npm run $SCRIPT_RUN'"
  },
  "dependencies": {
    "@apollo/subgraph": "^2.7.1",
    "@mercuriusjs/gateway": "^1.2.0",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/graphql": "^12.0.11",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mercurius": "^12.0.11",
    "@nestjs/microservices": "^10.3.1",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-fastify": "^10.3.0",
    "@nestjs/schedule": "^4.0.1",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/terminus": "^10.2.3",
    "@opentelemetry/api": "^1.8.0",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.51.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.51.0",
    "@opentelemetry/instrumentation-fastify": "^0.36.0",
    "@opentelemetry/instrumentation-graphql": "^0.40.0",
    "@opentelemetry/instrumentation-http": "^0.51.0",
    "@opentelemetry/resources": "^1.24.0",
    "@opentelemetry/sdk-metrics": "^1.24.0",
    "@opentelemetry/sdk-node": "^0.51.0",
    "@opentelemetry/sdk-trace-base": "^1.24.0",
    "@opentelemetry/semantic-conventions": "^1.24.0",
    "@opentelemetry/tracing": "^0.24.0",
    "@prisma/client": "^5.10.2",
    "@prisma/instrumentation": "^5.13.0",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dayjs": "^1.11.11",
    "dotenv": "^16.4.1",
    "dotenv-cli": "^7.3.0",
    "dottie": "^2.0.6",
    "flat": "^5.0.2",
    "google-auth-library": "^9.5.0",
    "google-spreadsheet": "^4.1.1",
    "googleapis": "^131.0.0",
    "graphql": "^16.8.1",
    "graphql-scalars": "^1.22.4",
    "graphql-subscriptions": "^2.0.0",
    "graphql-type-json": "^0.3.2",
    "graphql-ws": "^5.15.0",
    "i18n": "^0.15.1",
    "ioredis": "^5.3.2",
    "joi": "^17.12.2",
    "lodash": "^4.17.21",
    "mercurius": "^13.3.2",
    "moment": "^2.30.1",
    "nats": "^2.19.0",
    "nestjs-pino": "^4.0.0",
    "passport-jwt": "^4.0.1",
    "pino": "^8.20.0",
    "pino-http": "^9.0.0",
    "pino-pretty": "^10.3.1",
    "prisma-multischema": "^1.1.4",
    "prom-client": "^15.1.2",
    "redis": "^4.6.13",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "ts-morph": "^20.0.0",
    "undici": "^6.19.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.17",
    "@types/flat": "^5.0.5",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^9.0.11",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "prisma": "^5.10.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "prepare": "npm run format",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^@app/google_spreadsheet(|/.*)$": "<rootDir>/libs/google_spreadsheet/src/$1"
    }
  }
}
